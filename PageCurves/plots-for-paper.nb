Cell[CellGroupData[{

Cell["Maximum renyi-2", "Subsection",
 CellChangeTimes->{{3.8598983477430077`*^9, 
  3.8598983611789627`*^9}},ExpressionUUID->"d471d178-e2f9-46db-837f-\
19ea8730b987"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"S2max", "[", 
    RowBox[{"s_", ",", "r_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"Min", "[", 
     RowBox[{"r", ",", 
      RowBox[{"1", "-", "r"}]}], "]"}], 
    RowBox[{"Log", "@", 
     RowBox[{"Cosh", "[", 
      RowBox[{"2", "s"}], "]"}]}]}]}], ";"}]], "Input",
 CellLabel->
  "In[111]:=",ExpressionUUID->"558b1f95-a7d7-425f-89ca-a1307a447705"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 "Analytic page curve up to order ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["r", "dmax"], TraditionalForm]],ExpressionUUID->
  "17568256-6ac6-4d68-bfd2-c92ddf71aaff"]
}], "Subsection",
 CellChangeTimes->{{3.8598982593942633`*^9, 
  3.8598982756656322`*^9}},ExpressionUUID->"e86503f5-0721-4c2e-8054-\
2a41a9af4205"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"S2", "[", 
    RowBox[{"s_", ",", "r_", ",", 
     RowBox[{"dmax_:", "20"}]}], "]"}], ":=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"t2", "=", 
        SuperscriptBox[
         RowBox[{"Tanh", "[", 
          RowBox[{"2", "s"}], "]"}], "2"]}], ",", 
       RowBox[{"m", "=", 
        RowBox[{"Min", "[", 
         RowBox[{"r", ",", 
          RowBox[{"1", "-", "r"}]}], "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"m", " ", 
       RowBox[{"Log", "@", 
        RowBox[{"Cosh", "[", 
         RowBox[{"2", "s"}], "]"}]}]}], "+", 
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"d", "=", "2"}], "dmax"], 
       RowBox[{
        SuperscriptBox["m", "d"], 
        RowBox[{
         SubsuperscriptBox["\[Sum]", 
          RowBox[{"l", "=", 
           RowBox[{"Ceiling", "[", 
            RowBox[{"d", "/", "2"}], "]"}]}], 
          RowBox[{"d", "-", "1"}]], 
         RowBox[{
          FractionBox[
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"-", "1"}], ")"}], 
            RowBox[{"d", "-", "l"}]], "l"], 
          SuperscriptBox["t2", "l"], 
          RowBox[{"Binomial", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"2", "l"}], "-", "1"}], ",", 
            RowBox[{"l", "-", "1"}]}], "]"}], 
          RowBox[{"Binomial", "[", 
           RowBox[{"l", ",", 
            RowBox[{"d", "-", "l", "-", "1"}]}], "]"}], 
          FractionBox[
           RowBox[{
            RowBox[{"2", "l"}], "-", "d", "+", "1"}], 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"d", "-", "1"}], ")"}], "d"}]]}]}]}]}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->CompressedData["
